	"use strict";                                                                                                                                                                                                                                                 
	var that;                                                                                                                                                                                                                                                     
	sap.ui.controller("z2ui5compcont.ext.controller.ObjectPageExt", {                                                                                                                                                                                             
        onInit: function () {                                                                                                                                                                                                                                  
                                                                                                                                                                                                                                                               
			//Register onPageDataLoaded event                                                                                                                                                                                                                           
			this.extensionAPI.attachPageDataLoaded(function (oEvent) {                                                                                                                                                                                                  
				//Set controller object globally                                                                                                                                                                                                                           
				that = this;                                                                                                                                                                                                                                               
                                                                                                                                                                                                                                                               
				//Get selected context data                                                                                                                                                                                                                                
				this.oContext = oEvent.context;                                                                                                                                                                                                                            
				this.oContextData = this.oContext.getObject();                                                                                                                                                                                                             
				                                                                                                                                                                                                                                                           
				//Add component container to custom object page section                                                                                                                                                                                                    
				this.addCompContainer();                                                                                                                                                                                                                                   
				                                                                                                                                                                                                                                                           
			}.bind(this));                                                                                                                                                                                                                                              
		},                                                                                                                                                                                                                                                           
                                                                                                                                                                                                                                                               
        addCompContainer: function () {                                                                                                                                                                                                                        
			sap.ui.core.Component.create({                                                                                                                                                                                                                              
				name: "z2ui5",                                                                                                                                                                                                                                    
				settings: {                                                                                                                                                                                                                                                
					componentData: {                                                                                                                                                                                                                                          
						startupParameters: {                                                                                                                                                                                                                                     
							app_start: ["ZCL_2UI5_COMP_CONT_UI"],                                                                                                                                                                                                                   
							key1: [this.oContextData.Language],                                                                                                                                                                                                                     
							key2: [this.oContextData.Country],                                                                                                                                                                                                                      
                            key3: [this.oContextData.Nationality]                                                                                                                                                                                              
						}                                                                                                                                                                                                                                                        
					}                                                                                                                                                                                                                                                         
				}                                                                                                                                                                                                                                                          
			}).then(function (oComponent) {                                                                                                                                                                                                                             
				var oCompContainter = new sap.ui.core.ComponentContainer({                                                                                                                                                                                                 
					component: oComponent,                                                                                                                                                                                                                                    
					//height: "100px",                                                                                                                                                                                                                                        
					//width: "100%",                                                                                                                                                                                                                                          
					async: true                                                                                                                                                                                                                                               
				});                                                                                                                                                                                                                                                        
				                                                                                                                                                                                                                                                           
				// Add component container to your VBox                                                                                                                                                                                                                    
				var oVBox = that.getView().byId("VBoxId")                                                                                                                                                                                                                  
				oVBox.destroyItems();                                                                                                                                                                                                                                      
				oVBox.addItem(oCompContainter);                                                                                                                                                                                                                            
				                                                                                                                                                                                                                                                           
				//Overwrite default height of object page section                                                                                                                                                                                                          
				var oSection = that.getView().byId("z2ui5compcont::sap.suite.ui.generic.template.ObjectPage.view.Details::zc_comp_cont--AfterFacet::zc_comp_cont::GeneralInfo::Section");                                                                                  
				oSection.addStyleClass("customSectionHeight");                                                                                                                                                                                                             
				                                                                                                                                                                                                                                                           
			});                                                                                                                                                                                                                                                         
		}                                                                                                                                                                                                                                                            
    });                                                                                                                                                                                                                                                        
                                                                                                                                                                                                                                                               
