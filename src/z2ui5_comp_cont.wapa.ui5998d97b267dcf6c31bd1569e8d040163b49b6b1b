// Register ABAP2UI5 FLP integration for component container&nbsp;                                                                                                                                                                                             
jQuery.sap.registerModulePath("z2ui5", "/sap/bc/ui5_ui5/sap/z2ui5");                                                                                                                                                                         
                                                                                                                                                                                                                                                               
sap.ui.define(                                                                                                                                                                                                                                                 
    ["sap/suite/ui/generic/template/lib/AppComponent"],                                                                                                                                                                                                        
    function (Component) {                                                                                                                                                                                                                                     
        "use strict";                                                                                                                                                                                                                                          
                                                                                                                                                                                                                                                               
        return Component.extend("z2ui5compcont.Component", {                                                                                                                                                                                                   
            metadata: {                                                                                                                                                                                                                                        
                manifest: "json"                                                                                                                                                                                                                               
            }                                                                                                                                                                                                                                                  
        });                                                                                                                                                                                                                                                    
    }                                                                                                                                                                                                                                                          
);                                                                                                                                                                                                                                                             